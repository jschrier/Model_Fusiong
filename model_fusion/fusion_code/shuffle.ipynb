    }
   ],
   "source": [
    "#orginial sample difference\n",
    "diff1 <- same[1,2] - same[2,2]\n",
    "diff1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "# number of samples\n",
    "set.seed(7)\n",
    "NSample <- 10000\n",
    "ResultD <- rep(0, NSample)\n",
    "# the loop for averge diff\n",
    "for (i in 1:NSample) {\n",
    "    # shuffle the variable\n",
    "    var2 <- sample(var1, length(var1), replace = FALSE)\n",
    "    group2 <- sample(group1, length(group1), replace = FALSE)\n",
    "    # calculate the diff\n",
    "    res <- aggregate(x = var2, by = list(group2), FUN = mean)\n",
    "    diff <- res[1,2] - res[2,2]\n",
    "    ResultD[i] <- diff\n",
    "}"
   ]
  },

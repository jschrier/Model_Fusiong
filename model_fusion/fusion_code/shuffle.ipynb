   "source": [
    "#varables: 0 means no change on result, 1 mean change on result\n",
    "var1 <- c(1, rep(0, times = 25), rep(1, times = 18), rep(0, times = 17))\n",
    "#group: S mean in same region, D means in different region\n",
    "group1 <- c(rep('S', times = 26), rep('D', times = 35))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "same <- aggregate(x = var1, by = list(group1), FUN = mean)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
